page{
  height: 100%;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 80rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-guanbi:before {
  content: "\e71a";
}

.icon-xuanzhong:before {
  content: "\e723";
}

.image{
  align-items: center;
  display: flex;
  height: 100%;
  flex-direction: column;
  justify-content: space-around;
}
.editImage{
  width: 700rpx;
  border-radius: 10rpx;
  border: 1rpx solid black;
}
.editText{
  width: 700rpx;
  height: 400rpx;
  border-radius: 10rpx;
  padding: 10rpx;
  box-sizing: border-box;
  border: 1rpx solid;
}

.choice{
  display: flex;
  width: 700rpx;
  justify-content: space-between;
}